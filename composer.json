{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-openssl": "*",
        "abordage/laravel-html-min": "^0.2.2",
        "blade-ui-kit/blade-icons": "^1.8",
        "caseyamcl/toc": "^4.0",
        "guzzlehttp/guzzle": "^7.9.3",
        "intervention/image-laravel": "^1.5.6",
        "laravel/framework": "^12.24",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.6.4",
        "spatie/laravel-medialibrary": "^11.13",
        "spatie/laravel-sitemap": "^7.3.6",
        "spatie/laravel-tags": "^4.10",
        "spatie/schema-org": "^3.23.1",
        "web-token/jwt-framework": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16.0",
        "fakerphp/faker": "^1.24.1",
        "laravel/pint": "^1.24.0",
        "laravel/sail": "^1.44.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "phpunit/phpunit": "^12.3.4"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/Functions.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "pint",
        "test:lint": "pint --test"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
